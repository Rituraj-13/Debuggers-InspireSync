{
    "manifest_version": 3,
    "name": "Updated_IS",
    "description": "An Open Source chrome extension for boosting productivity.",
    "version": "1.1",
    "action": {
        "default_popup": "index.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["./scripts/websiteblocker.js"]
        }
    ],
    "permissions": [
        "storage",
        "activeTab"
    ]
}